{
  "name": "figma-style-parser",
  "version": "1.0.0",
  "description": "Component library generator.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "fsp": "dist/index.js"
  },
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "build": "tsc",
    "prepublish": "tsc",
    "local:install": "npm i -g && fsp",
    "docs:gen": "jsdoc src -r -c ./jsdoc.config.json -d docs",
    "test": "jest --coverage",
    "test:clean": "jest --clearCache",
    "test:init": "jest --init"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "parser",
    "figma",
    "style",
    "generator",
    "components"
  ],
  "author": "Maksim Sandybekov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ExLeonem/figma-style-parser/issues"
  },
  "homepage": "https://github.com/ExLeonem/figma-style-parser#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "commander": "^6.1.0",
    "configstore": "^5.0.1",
    "envinfo": "^7.7.3",
    "prompts": "^2.3.2",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "better-docs": "^2.3.2",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
